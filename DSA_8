a=[5,4,3,2,1]
n=len(a)
ans=[]
for i in range(n-1):
    temp=float(-1)
    for j in range(i+1,n):
        temp=max(temp,a[j])
    ans.append(temp)
ans.append(-1)
print(ans)

'''METHOD-2''

 n=len(arr)
        
        rmax=-1
        for i in range(n-1,-1,-1):
            temp=arr[i]
            arr[i]=rmax                ''' O(N) --- TIME COMPLEXITY '''''
            rmax=max(rmax,temp)
        return arr
